layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">

  <!-- For the 'index.md' page -->
  <url>
    <loc>{{ site.url }}/</loc>
    {% if site.sitemap.lastmod %}
      <lastmod>{{ site.sitemap.lastmod | date: "%Y-%m-%d" }}</lastmod>
    {% else %}
      <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    {% endif %}
    {% if site.sitemap.changefreq %}
      <changefreq>{{ site.sitemap.changefreq }}</changefreq>
    {% else %}
      <changefreq>monthly</changefreq>
    {% endif %}
    {% if site.sitemap.priority %}
      <priority>{{ site.sitemap.priority }}</priority>
    {% else %}
      <priority>0.5</priority>
    {% endif %}
  </url>

  <!-- For the 'research.md' page -->
  <url>
    <loc>{{ site.url }}/research.html</loc>
    {% if page.sitemap.lastmod %}
      <lastmod>{{ page.sitemap.lastmod | date: "%Y-%m-%d" }}</lastmod>
    {% else %}
      <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    {% endif %}
    {% if page.sitemap.changefreq %}
      <changefreq>{{ page.sitemap.changefreq }}</changefreq>
    {% else %}
      <changefreq>monthly</changefreq>
    {% endif %}
    {% if page.sitemap.priority %}
      <priority>{{ page.sitemap.priority }}</priority>
    {% else %}
      <priority>0.3</priority>
    {% endif %}
  </url>

</urlset>
